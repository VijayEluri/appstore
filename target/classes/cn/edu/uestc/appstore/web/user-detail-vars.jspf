<div id="pageVariables">
    <c:catch>
        <c:set var="userId" value="${form.userId}" scope="request"/>
    </c:catch>
    <c:catch>
        <c:set var="username" value="${form.username}" scope="request"/>
    </c:catch>
    <c:catch>
        <c:set var="password" value="${form.password}" scope="request"/>
    </c:catch>
    <c:catch>
        <c:set var="realName" value="${form.realName}" scope="request"/>
    </c:catch>
</div>
